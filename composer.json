{
    "name": "webgriffe/amp-elasticsearch",
    "description": "A non-blocking ElasticSearch client for PHP based on Amp. Fork of webgriffe/amp-elasticsearch.",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "amphp/http-client": "^5.0",
        "amphp/amp": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Webgriffe\\AmpElasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webgriffe\\AmpElasticsearch\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.19.5"
    }
}
